<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Sekti šeimininką kai jis mobilizuotas</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Sekti šeimininką kai jis dirba lauke (medžioja arba jaukina gyvūnus)</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters. -->
  <Hunt.headerTip>Pažymėk gyvūnus, kuriuos tavo medžiotojai medžios.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>Paleisti į gamtą</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Papjauti</Slaughter.headerTip>
  
  <!-- EN: Sterilize -->
  <Sterile.headerTip>Sterilizuoti</Sterile.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Jaukinti laukinį gyvūną.</Tame.headerTip>
  
</LanguageData>